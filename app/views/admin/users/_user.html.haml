%tr
  %td= h user
  %td= user.email
  %td= user.role
  %td
    = link_to image_tag('icon_small_show.png'), admin_user_path(user)
    = link_to image_tag('icon_small_edit.png'), edit_admin_user_path(user)
    = link_to image_tag('icon_small_delete.png'), :confirm => 'Are you sure?', :method => :delete